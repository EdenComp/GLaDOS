fn main() {
    int height = 123;
    int i = 0;
    int j = 0;
    int k = 0;

    while (i < height) {

        j = 0;
        while (j < i) {
            print(" ");
            j = j + 1;
        }

        k = 0;

        while (k < 2 * (height - i) - 1) {
            print("*");
            k = k + 1;
        }

        print("\n");
        i = i + 1;
    }

    return 0;
}

main();
