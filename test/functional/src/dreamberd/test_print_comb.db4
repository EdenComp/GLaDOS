fn print_comb_2() {
    int c = 0;
    int d = 0;
    int u = 0;

    while (c <= 7) {
        d = c + 1;
        while (d <= 8) {
            u = d + 1;
            while (u <= 9) {
                print(c + "" + d + "" + u);
                if ((c != 7) || (d != 8) || (u != 9)) {
                    print(", ");
                }
                u++;
            }
            d++;
        }
        c++;
    }
}

fn main() {
    print_comb_2();
}

main();
